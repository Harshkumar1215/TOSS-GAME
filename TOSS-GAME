class Toss_Game
{
public static void main(String args[])
{
int i,c,h=0,t=0;
double d;
for(i=1;i<=20;i++)
{
d=Math.random()*2;
c=(int)d;
if(c==1)
{
h++;
}
else
{
t++;
}
}
System.out.println("Number of times head obtained="+h);
System.out.println("number of times tail obtained="+t);
if(h>t){
    System.out.println("HEAD is winner ");
}
 else if(h==t){

System.out.println("Match tie  ");
}
else{
    System.out.println("Tel  is winner ");
}
}
}
